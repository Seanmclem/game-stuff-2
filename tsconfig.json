{
  "compilerOptions": {
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "skipDefaultLibCheck": true,
    "target": "ESNext",
    "verbatimModuleSyntax": true,
    "strict": true,
    "noEmit": true,
    "moduleResolution": "node",
    "strictNullChecks": false,
    "jsx": "preserve",
    "declaration": true,
    "lib": [
      "es2022",
      "dom"
    ],
    "types": [
      "@react-three/fiber"
    ]
  },
  "include": [
    "."
  ],
  "exclude": [
    "node_modules"
  ]
}
